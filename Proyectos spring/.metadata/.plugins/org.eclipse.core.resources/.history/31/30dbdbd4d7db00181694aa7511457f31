package pe.edu.upeu.sprintemplate.test;

import org.springframework.context.support.ClassPathXmlApplicationContext;

import pe.edu.upeu.sprintemplate.entity.Usuario;
import pe.edu.upeu.sprintemplate.entity.producto;
import pe.edu.upeu.sprintemplate.repository.productoRepository;

public class AppConexion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ClassPathXmlApplicationContext context = new ClassPathXmlApplicationContext("root-context.xml");
		productoRepository repo = context.getBean("productoRepository",productoRepository.class);
		producto u = new producto(1,1,"Parachoques",
		20,1,123,"1"
				);
		repo.save(u);
		context.close();
	}

}
